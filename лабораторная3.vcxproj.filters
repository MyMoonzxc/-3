#include <stdio.h>
#include <locale.h>

int main() {
    setlocale("LC_CTYPE,"RUS");
    int years;
    
    printf("Сколько вам лет? ");
    scanf("%d", &years);
    
    int days = years * 365;
    
    printf("Вы прожили примерно %d дней!\n", days);
    printf("Это около %d недель\n", days / 7);
    
    return 0;
}
